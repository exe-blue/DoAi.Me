    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.183.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.5.12",
    "jsdom": "^28.0.0",
    "postcss": "^8.4.49",
    "supabase": "^2.76.15",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
