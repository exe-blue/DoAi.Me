{
  "name": "doai-me",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev --filter=./apps/web --filter=./apps/desktop",
    "build": "turbo run build",
    "build:desktop": "pnpm --filter @doai/desktop build",
    "dist:desktop": "pnpm --filter @doai/desktop dist",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "dev:next": "next dev",
    "build:next": "next build",
    "start": "next start",
    "lint:next": "next lint",
    "test:next": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "node tests/e2e-local.js",
    "test:e2e:watch": "node tests/e2e-youtube-watch.js",
    "test:api": "node tests/run-api-tests.js",
    "agent:dev": "cd agent && npm run dev",
    "agent:start": "cd agent && npm start",
    "db:link": "npx supabase link --project-ref vyfxrplzhskncigyfkaz",
    "db:push": "npx supabase db push",
    "db:verify": "npx supabase db execute --file supabase/verify_schema.sql",
    "clean": "rm -rf .next",
    "clean:cache": "rm -rf .next/cache",
    "register-channels": "node scripts/register-channels.mjs",
    "deploy": "node scripts/deploy.mjs",
    "health": "node scripts/pm2-health-check.mjs",
    "rollback": "node scripts/rollback.mjs"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.4",
